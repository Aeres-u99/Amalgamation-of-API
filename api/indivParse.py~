    content_axu = axu.getData_by_lat_lon(lat,lon)
    return content_axu
def axu_parse_acc_city(city):
    content_axu = axu.getData_by_City(city)
    return content_axu
def axu_parse_acc_postal_code(pincode):
    content_axu = axu.getData_by_Postalcode(pincode)
    return content_axu
def axu_parse_acc_ip_add(ip_address):
