import ApiXu.ApiXu ais axu
import DarkSky.DarkSky as ds
import OpenWeather.OpenWeather as ow
import WeatherBit.WeatherBit as wb
import json

def ds_parse_lat_lon(lat,lon):
    content_ds = ds.getData_by_lat_lon(lat,lon)
    return content_ds
def ds_parse_acc_city(city):
    content_ds = ds.getData_by_City(city)
    return content_ds
def ds_parse_acc_postal_code(pincode):
    content_ds = ds.getData_by_Postalcode(pincode)
    return content_ds
def ds_parse_acc_ip_add(ip_address):
    content_ds = ds.getData_by_ip(ip_address)
    return content_ds


def ow_parse_lat_lon(lat,lon):
    content_ow = ow.getData_by_lat_lon(lat,lon)
    return content_ow
def ow_parse_acc_city(city):
    content_ow = ow.getData_by_City(city)
    return content_ow
def ow_parse_acc_postal_code(pincode):
    content_ow = ow.getData_by_Postalcode(pincode)
    return content_ow
def ow_parse_acc_ip_add(ip_address):
    content_ow = ow.getData_by_ip(ip_address)
    return content_ow


